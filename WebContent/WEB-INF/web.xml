<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>saylovewall</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <filter>
    <filter-name>Authorize</filter-name>
    <filter-class>com.yiban.filter.AuthorizeFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>Authorize</filter-name>
    <url-pattern>/user/*</url-pattern>
  </filter-mapping>
  <servlet>
    <servlet-name>LoveLinkServlet</servlet-name>
    <servlet-class>com.yiban.servlet.LoveLinkServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoveLinkServlet</servlet-name>
    <url-pattern>/lls</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>UserLikeServlet</servlet-name>
    <servlet-class>com.yiban.servlet.UserLikeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserLikeServlet</servlet-name>
    <url-pattern>/uls</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AppServlet</servlet-name>
    <servlet-class>com.yiban.servlet.AppServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AppServlet</servlet-name>
    <url-pattern>/as</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>DiscussServlet</servlet-name>
    <servlet-class>com.yiban.servlet.DiscussServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DiscussServlet</servlet-name>
    <url-pattern>/ds</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AdminServlet</servlet-name>
    <servlet-class>com.yiban.servlet.AdminServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AdminServlet</servlet-name>
    <url-pattern>/vadmin</url-pattern>
  </servlet-mapping>
  <filter>
    <filter-name>AdminFilter</filter-name>
    <filter-class>com.yiban.filter.AdminFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>AdminFilter</filter-name>
    <url-pattern>/admin/*</url-pattern>
  </filter-mapping>
</web-app>